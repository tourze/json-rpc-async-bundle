{
  "name": "tourze/json-rpc-async-bundle",
  "description": "JsonRPC异步实现",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/cache-contracts": "^3",
    "tourze/json-rpc-core": "^0.0.6",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/doctrine-snowflake-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/symfony-async-bundle": "0.0.*",
    "tourze/symfony-schedule-entity-clean-bundle": "0.0.*",
    "psr/log": "^3|^2|^1",
    "psr/simple-cache": "^1.0|^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCAsyncBundle\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}